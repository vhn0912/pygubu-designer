<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="mainwindow">
    <property name="geometry">300x180</property>
    <property name="height">200</property>
    <property name="title" translatable="yes">Button Commands Example</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="mainframe">
        <property name="height">250</property>
        <property name="padding">10</property>
        <property name="width">250</property>
        <layout manager="pack" />
        <child>
          <object class="ttk.Label" id="label_1">
            <property name="anchor">e</property>
            <property name="padding">10</property>
            <property name="text" translatable="yes">Button command test!!</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="my_button">
            <property name="command" type="command" cbtype="simple" args="">on_my_button_clicked</property>
            <property name="text" translatable="yes">Click me</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_2">
            <property name="command" type="command" cbtype="simple" args="">on_button2_clicked</property>
            <property name="text" translatable="yes">Button 2</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
